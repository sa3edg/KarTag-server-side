package com.kartag.common.dao.jdbc;

import java.util.ArrayList;

public class JQuery {

	private String Query = "";
	private ArrayList<Object> queryParameters = new ArrayList<Object>();
	private ArrayList<String> queryResult = new ArrayList<String>();
	
	public JQuery(String query)
	{
		this.Query = query;
	}
	public void setQuery(String query) {
		Query = query;
	}
	public String getQuery() {
		return Query;
	}
	public void setQueryParameters(ArrayList<Object> queryParameters) {
		this.queryParameters = queryParameters;
	}
	public ArrayList<Object> getQueryParameters() {
		return queryParameters;
	}
	public void setQueryResult(ArrayList<String> queryResult) {
		this.queryResult = queryResult;
	}
	public ArrayList<String> getQueryResult() {
		return queryResult;
	}
	
}
